import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
            Scanner in=new Scanner(System.in);
      long a=in.nextInt();

      long sum=0;
      for(int i=0; i<a; i++){
        sum+=in.nextInt();
      }
      System.out.println(sum);
    }
  }
